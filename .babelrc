    "@babel/preset-es2017",
    "@babel/preset-es2016",
    ["@babel/preset-es2015", { "modules": false }],
    "@babel/preset-react"
  ],
  "plugins": ["@babel/plugin-proposal-class-properties"],
  "env": {
    "test": {
      "presets": [
